<% if user_signed_in? %>
  <%= form_with url: search_path, method: :get, local: true do |f| %>
    <div class="form-group ml-5">
      <%= f.text_field :content, class: "mb-1" %>
      <%= f.select :model, options_for_select({"投稿" => "post", "ユーザー" => "user"}) %>
      <%= f.select :method, options_for_select({"部分一致" => "partial", "完全一致" => "perfect"}) %>
      <%= f.submit "検索", class: "btn btn-primary mt-1" %>
    </div>
  <% end %>
<% end %>